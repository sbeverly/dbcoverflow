<%= form_for(@user) do |f| %>

	<p>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</p>

	<p>
		<%= f.label :password %>
		<%= f.text_field :password %>
	</p>

	<p>
		<%= f.label :password_confirmation %>
		<%= f.text_field :password_confirmation %>
	</p>

	<p>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</p>

	<%= f.submit "Create User" %>
		
<% end %>
  